{
  "name": "josh865/pulsis-theme",
  "description": "A UI theme for PULSIS web projects.",
  "type": "library",
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^7.0|^8.0"
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "Pulsis\\Theme\\PulsisThemeProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Pulsis\\Theme\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
